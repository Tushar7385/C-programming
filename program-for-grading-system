//GRADING SYSTEM

#include<stdio.h>
int main()
{ 
    int x;
    printf("Enter marks scored\n");
    scanf("%d",&x);
    if((x>90)&&(x<=100))
        printf("Grade A");
    else if((x<=90)&&(x>80))
        printf("Grade B");
    else if((x<=80)&&(x>70))
        printf("Grade C");
    else if((x<=70)&&(x>60))
        printf("Grade D");
    else 
        printf("Grade F");
}
