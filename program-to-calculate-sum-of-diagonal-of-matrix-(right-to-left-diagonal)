//VOID FUNCTION (SUM OF DIAGONAL OF MATRIX ) (RIGHT TO LEFT DIAGONAL)
#include <stdio.h>
void matrix();
int main()
{
    matrix();
    
}

void matrix()
{
    int i,j,A[3][3],B[3][3],C[3][3],sum;
    
    
        for(i=0;i<=2;i++)
            for(j=0;j<=2;j++)
            {    printf("Enter  integer");
                scanf("%d",&A[i][j]);
            }
        for(i=0;i<=2;i++)
            for(j=0;j<=2;j++)
            if(i+j==2)
                sum=sum+A[i][j];
            printf("%d\n",sum);    
            

            
                
                
        
        
    
    
}
