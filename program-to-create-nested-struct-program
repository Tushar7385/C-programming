#include <stdio.h>


struct complex{
    int imag;
    float real;
    
};
struct number{
    struct complex comp;
    int integer;
    
}num1,num2;
int main()
{
    num1.comp.imag=25;
    num1.comp.real=565;
    num1.integer=757;
    
    printf("%d\n",num1.comp.imag);
    printf("%.2f\n",num1.comp.real);
    printf("%d",num1.integer);
}

