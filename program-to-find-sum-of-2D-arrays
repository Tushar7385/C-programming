//sum using 2D arrays
#include <stdio.h>

int main()
{
    int A[3][3],i,r,c,sum;
    
    for(r=0;r<=2;r++)
        for(c=0;c<=2;c++)
            scanf("%d",&A[r][c]);
    for(r=0;r<=2;r++)
        for(c=0;c<=2;c++)
            sum=sum+A[r][c];
            
            printf("%d is the sum",sum);
    
    
}
