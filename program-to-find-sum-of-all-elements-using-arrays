//sum of all elements using arrays
#include <stdio.h>

int main()
{
    int A[10],i,sum;
    
    
      for(i=0;i<=5;i++)
        scanf("%d",&A[i]);
      for(i=0;i<=5;i++)
        printf("%d    ",A[i]);
      for(i=0;i<=5;i++)
        sum=sum+A[i];
        
        printf("%d",sum);
        
        
}
