//greatest number using arrays
#include <stdio.h>

int main()
{
    int A[10],i,max;
    
    
      for(i=0;i<=5;i++)
        scanf("%d",&A[i]);
    max=A[0];    
      for(i=0;i<=5;i++)
        if(A[i]>max)
            max=A[i];
            
        printf("%d is the greatest number",max);    
}

