
//even and odd no.s
#include <stdio.h>

int main()
{
    int A[10],i,sum;
    
    
      for(i=0;i<=5;i++)
        scanf("%d",&A[i]);
      for(i=0;i<=5;i++)
       if(A[i]%2==0)
        printf("%d is even\n",A[i]);
       else if(A[i]%2==1) 
        printf("%d is odd\n",A[i]);
        
        
}
