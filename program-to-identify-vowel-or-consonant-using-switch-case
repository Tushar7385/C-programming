//take user inpur  and print whether vowel or consonant (switch case)
#include <stdio.h>

int main()
{
    char alphabet ='L';
    
    switch(alphabet)
    {
        case 'A':
        case 'E':
        case 'O':
        case 'I':
        case 'U':
        case 'a':
        case 'e':
        case 'o':
        case 'u':
        case 'i':
            printf("the given alphabet is a vowel\n");
            break;
        default:
            printf("the given alphabet is a consonant\n");
            
        
        
        
        
        
        
    }
    
    
        
    
}
