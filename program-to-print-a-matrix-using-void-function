#include <stdio.h>
void matrix();
int main()
{
    matrix();
    
}

void matrix()
{
    int i,j,A[3][3],B[3][3],C[3][3];
    
    
        for(i=0;i<=2;i++)
            for(j=0;j<=2;j++)
            {    printf("Enter  integer");
                scanf("%d",&A[i][j]);
            }
        for(i=0;i<=2;i++)
            for(j=0;j<=2;j++)
            {printf("Enter an integer");
                scanf("%d",&B[i][j]);
            }
        for(i=0;i<=2;i++)
            for(j=0;j<=2;j++)    
            { C[i][j]= A[i][j]+B[i][j] ;}
            
            for(i=0;i<=2;i++)
            { for(j=0;j<=2;j++) 
              printf("%d ",C[i][j]);
              printf("\n");          }
            
                
                
        
        
    
    
}
