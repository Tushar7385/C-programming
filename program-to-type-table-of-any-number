#include <stdio.h> 
int main() 
{
int x=2; //declaration of variables. int is data type , it tells us size , type of value and range of values
  
   printf("%d*1=%d\n",x,x*1); //%d is format specifier
   printf("%d*2=%d\n",x,x*2);
   printf("%d*3=%d\n",x,x*3);
   printf("%d*4=%d\n",x,x*4);
   printf("%d*5=%d\n",x,x*5);
   printf("%d*6=%d\n",x,x*6);
   printf("%d*7=%d\n",x,x*7);
   printf("%d*8=%d\n",x,x*8);
   printf("%d*9=%d\n",x,x*9);
   printf("%d*10=%d\n",x,x*10);
}
